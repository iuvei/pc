.money-wrap
  .top-wrap
    .title 千万奖池 一触即发
    .desc 高品质游戏 仅此一家 您值得拥有
  .list-wrap(@mouseenter="onListEnter" @mouseleave="onListLeave")
    ul(:style="{ top: listTop+'px' }")
      li(v-for="item, index in list" :key="index")
        span.user {{ item.user }}
        span.game {{ item.game }}
        span.money {{ item.money }}
      li(v-for="item, index in list" :key="index+10")
        span.user {{ item.user }}
        span.game {{ item.game }}
        span.money {{ item.money }}
  .ad-wrap
    img(src="./img/activity.png" @click="onActivityClick")
